#! /bin/sh

export LANG=C
export LC_CTYPE=C
export LC_COLLATE=C

#set -o xtrace

prefix=@prefix@
. @sysconfdir@/gfarm-s3.conf
. $GFARM_S3_BIN/gfarm-s3-subr

gfarm_s3_local_user_map=$GFARMS3_LOCAL_USER_MAP
if [ $# = 1 ]; then
	gfarm_s3_local_user_map="$1"
fi

if ! configtest_all $gfarm_s3_local_user_map; then
	echo configtest failed
	exit 1
fi
