#! /bin/sh

export LANG=C
export LC_CTYPE=C
export LC_COLLATE=C

#set -o xtrace

prefix=@prefix@
. @sysconfdir@/gfarm-s3.conf
. $GFARM_S3_BIN/gfarm-s3-subr

USAGE="usage: $0 old new"

if [ $# != 3 ]; then
	echo $USAGE 1>&2
	exit 1
fi

old="$1"
new="$2"
portmap="$3"

users_subject_to_be_stopped "$old" "$new" "$portmap"
