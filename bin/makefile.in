prefix = @prefix@
exec_prefix = @exec_prefix@
bindir = @bindir@
libdir = @libdir@
libexecdir = @libexecdir@
INSTALL = install

scripts= \
	gfarm-s3-configtest \
	gfarm-s3-diff-local-usermap \
	gfarm-s3-generate-usermap-hpci \
	gfarm-s3-login \
	gfarm-s3-server \
	gfarm-s3-minio \
	gfarm-s3-readconf \
	gfarm-s3-sharedsecret-password \
	gfarm-s3-update-reverse-proxy \
	gfarm-s3-useradd \
	gfarm-s3-viusermap

generated_scripts= \
	gfarm-s3-configtest \
	gfarm-s3-diff-local-usermap \
	gfarm-s3-login \
	gfarm-s3-server \
	gfarm-s3-minio \
	gfarm-s3-readconf \
	gfarm-s3-sharedsecret-password \
	gfarm-s3-update-reverse-proxy \
	gfarm-s3-useradd \
	gfarm-s3-viusermap

libexec_files = \
	gfarm-s3-webui \
	gfarm-s3-router

all:

install:
	for i in $(scripts); do $(INSTALL) -D -c -m 555 $$i $(bindir)/$$i; done
	for i in $(libexec_files); do $(INSTALL) -D -c -m 555 $$i $(libexecdir)/$$i; done
	$(INSTALL) -D -c -m 644 gfarm-s3-subr $(libdir)/gfarm-s3-subr

clean:

distclean:
	$(RM) -f $(generated_scripts)
	$(RM) -f makefile
