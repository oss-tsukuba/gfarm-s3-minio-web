### Runas_Alias GFARMS3 = ALL, !root
Runas_Alias GFARMS3 = %gfarms3

@HTTPD_USER@ ALL=(GFARMS3) NOPASSWD: @GFARMS3_BIN@/gfarm-s3-server
@HTTPD_USER@ ALL=(GFARMS3) NOPASSWD: /usr/bin/grid-proxy-info
@HTTPD_USER@ ALL=(GFARMS3) NOPASSWD: /usr/bin/grid-proxy-init
@HTTPD_USER@ ALL=(GFARMS3) NOPASSWD: /usr/bin/myproxy-logon
@HTTPD_USER@ ALL=(GFARMS3) NOPASSWD: /usr/local/bin/gfkey
@HTTPD_USER@ ALL=(root) NOPASSWD: /usr/sbin/apachectl
