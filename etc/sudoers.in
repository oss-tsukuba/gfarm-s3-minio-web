### Runas_Alias GFARMS3 = ALL, !root
Runas_Alias GFARMS3 = %gfarms3

@WSGI_USER@ ALL=(GFARMS3) NOPASSWD: @GFARMS3_BIN@/gfarm-s3-server
## --with-globus=/usr
@WSGI_USER@ ALL=(GFARMS3) NOPASSWD: /usr/bin/grid-proxy-info
@WSGI_USER@ ALL=(GFARMS3) NOPASSWD: /usr/bin/grid-proxy-init
## --with-myproxy=/usr
@WSGI_USER@ ALL=(GFARMS3) NOPASSWD: /usr/bin/myproxy-logon
## --with-gfarm=/usr/local
@WSGI_USER@ ALL=(GFARMS3) NOPASSWD: /usr/local/bin/gfkey
## --with-apache=/usr
@WSGI_USER@ ALL=(root) NOPASSWD: /usr/sbin/apachectl
